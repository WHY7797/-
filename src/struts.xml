<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
	<struts>
	
	
<package name="student" namespace="/" extends="struts-default" >
	<action name="*_*" class="com.action.{1}" method="{2}">
		<result name="successStu">/stu.jsp</result> 
		<result name="successTea">/tea.jsp</result> 
		<result name="error">/error.jsp</result>   
		<result name="teaAll">/query.jsp</result> 
		<result name="stuAll">/query1.jsp</result> 
	</action>
	<action name="queryu" class="com.action.UserAction" method="queryu">
		<result name="success">/queryu.jsp</result> 
		<result name="error">/error.jsp</result>   
	</action>
	<action name="update" class="com.action.UserAction" method="update">
		<result name="success">/updateu.jsp</result> 
		<result name="error">/error.jsp</result>   
	</action>
	<action name="updateu" class="com.action.UserAction" method="updateu">
		<result name="usec">/updateu.jsp</result> 
		<result name="error">/error.jsp</result>   
	</action>
  </package>
 
  </struts>